<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="СlinicReception.Views.DbAdminView">
	<Panel>
		<StackPanel>
			<TabControl>
				<TabItem Header="Формирование расписания врачей" VerticalContentAlignment="Center">
					<StackPanel>
						<TextBox  Watermark="Врач" />
						<ComboBox SelectedIndex="0">
							<ComboBoxItem>Терапевт</ComboBoxItem>
							<ComboBoxItem>Хирург</ComboBoxItem>
							<ComboBoxItem>Уролог</ComboBoxItem>
							<ComboBoxItem>Гинеколог</ComboBoxItem>
							<ComboBoxItem>Дерматолог</ComboBoxItem>
							<StackPanel>
								<Calendar Name="DisplayDatesCalendar" SelectionMode="SingleDate"/>
							</StackPanel>
						</ComboBox>
					</StackPanel>
				</TabItem>
			</TabControl>
		</StackPanel>
	</Panel>
</UserControl>
