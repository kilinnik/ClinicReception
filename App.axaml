<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:СlinicReception"
			  xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             x:Class="СlinicReception.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
		<themes:MaterialTheme BaseTheme="Dark" PrimaryColor="Purple" SecondaryColor="Lime" />
		<StyleInclude Source="avares://СlinicReception/Assets/Icons.axaml" />
		<StyleInclude Source="avares://Material.Icons.Avalonia/App.xaml"></StyleInclude>
		<StyleInclude Source="avares://DialogHost.Avalonia/Styles.xaml"/>
    </Application.Styles>
</Application>
